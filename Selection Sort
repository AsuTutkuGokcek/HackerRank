// Selection Sort
    for(int i=0; i<arr.size(); i++){
        int inHand = arr.get(i);
        int Temp = inHand;
        int Tempindex = i;
        for(int j=i; j<arr.size()-i; j++){
            if(inHand>arr.get(j)){
                inHand=arr.get(j);
                Tempindex=j;
            }
        }
        arr.set(i, inHand);
        arr.set(Tempindex, Temp);
    }
